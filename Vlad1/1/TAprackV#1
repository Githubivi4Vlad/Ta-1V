#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    double x, y, z;
    double a = -2.004, b = 0.87;

    x = pow(pow(a + b, 2), -4.1);
    y = pow(atan(1 / b), 3);
    double xy = x * y;
    z = (exp(-xy) + 17.4) / pow((pow(sin(xy), 2)), 1/3);
    cout << x << "  " << y << "  " << z << endl;

}

